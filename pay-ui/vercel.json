{
  "installCommand": "cd .. && npm install && cd bank-ui && npm install",
  "buildCommand": "cd ../bank-api && npm run build && cd ../bank-ui && npm run build:testnet",
  "outputDirectory": "dist",
  "framework": null,
  "git": {
    "deploymentEnabled": {
      "testnet": true,
      "main": false
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/keys/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/zkir/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
